<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
   

    @Styles.Render("~/css/bootstrap.css")
    @Styles.Render("~/css/carousel.css")

