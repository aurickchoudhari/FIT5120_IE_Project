<!-- Script to embed tableau reports -->
<div id="tableauViz" style="width:100%;margin-top:50px"></div>
<script>

    var viz, workbook, activeSheet;
    $(document).ready(function () {
        initializeViz();
    });


    function initializeViz() {


        var placeholderDiv = document.getElementById("tableauViz");
        //Options to show the sheet
        var options = {

            width: "100%",
            height: "500px",
            onFirstInteractive: function () {
                workbook = viz.getWorkbook();
                activeSheet = workbook.getActiveSheet();
            }

        };
        //URL to the sheet

        var url = 'https://public.tableau.com/views/typeofparticipationinnaturalenvironmentrelatedactivities/Sheet1?:embed=y&:display_count=yes'

        viz = new tableauSoftware.Viz(placeholderDiv, url, options);

    }
</script>


